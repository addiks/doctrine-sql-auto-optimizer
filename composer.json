{
    "name": "addiks/doctrine-sql-auto-optimizer",
    "description": "Zero-configuration drop-in tool to automatically optimize SQL queries issued through doctrine",
    "type": "library",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1.0",
        "doctrine/dbal": "^3.5",
        "webmozart/assert": "^1.11",
        "addiks/stored-sql": "dev-master",
        "monolog/monolog": "3.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.29",
        "phpstan/phpstan": "^1.8",
        "phpstan/phpstan-doctrine": "^1.3"
    },
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Addiks\\DoctrineSqlAutoOptimizer\\": "php/"
        }
    },
    "authors": [
        {
            "name": "Gerrit Addiks",
            "email": "gerrit@addiks.de"
        }
    ]
}
